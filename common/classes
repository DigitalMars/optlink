.XLIST

CLASS_STRUCT	STRUC

_C_NEXT_HASH_GINDEX	DD	?	;NEXT CLASS - HASH ORDER
_C_NEXT_CLASS_GINDEX	DD	?	;PTR TO NEXT CLASS RECORD

_C_FIRST_SEG_GINDEX	DD	?	;PTR TO FIRST SEGMENT THIS CLASS
_C_LAST_SEG_GINDEX	DD	?	;PTR TO LAST SEGMENT THIS CLASS
if	fg_rom
_C_BASE 		DD	?	;BASE ADDRESS IF ORG'ED
_C_PHASE		DD	?	;REAL CLASS PTR
endif
if	any_overlays
_C_SECTION_GINDEX	DD	?	;ASSIGNED SECTION
_C_PLTYPE		DB	?	;OVERLAY FLAGS (PLTYPES)
			DB	?	;AVAIL
endif
_C_TYPE_FLAG		DB	?	;SEG_FLAGS
_C_ORDER_FLAG		DB	?	;ORDERING FLAGS
			DW	?

_C_HASH			DD	?	;FOR FASTER SEARCHES
_C_TEXT			DD	?	;CLASS NAME

CLASS_STRUCT	ENDS

.LIST

