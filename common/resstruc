.XLIST

RESNAME_STRUCT		STRUC

_RN_NEXT_HASH_GINDEX	DD	?		;NEXT NAME SAME HASH
_RN_NEXT_RN_GINDEX	DD	?		;NEXT, RESNAME ORDER
_RN_ALPHA_ORDER		DD	?		;ALPHABETICAL ORDER
_RN_OFFSET		DD	?		;OFFSET IN RESNAME TABLE
_RN_UNITEXT		DD	?		;START OF UNICODE STRING

RESNAME_STRUCT		ENDS


RESTYPE_STRUCT		STRUC

_RT_NEXT_HASH_GINDEX	DD	?		;NEXT RESOURCE TYPE THIS HASH
_RT_NEXT_RT_GINDEX	DD	?		;NEXT TYPE WITH SAME BYORDN VS BYNAME
_RT_N_RTN_BYNAME	DD	?		;NUMBER OF RTN'S THIS TYPE BYNAME
_RT_N_RTN_BYORD		DD	?		;NUMBER OF RTN'S THIS TYPE BY ORDINAL
_RT_RTN_BYNAME_GINDEX	DD	?		;LAST RTN
_RT_RTN_BYORD_GINDEX	DD	?		;
_RT_ID_GINDEX		DD	?		;1-15, & USER DEFINED...  <64K IS ID, >64K IS RESNAME_GINDEX

;_RT_ALPHA_ORDER		EQU	(_RT_NEXT_HASH_GINDEX)

RESTYPE_STRUCT		ENDS


RES_TYPE_NAME_STRUCT	STRUC

_RTN_NEXT_HASH_GINDEX	DD	?		;NEXT RTN SAME HASH
_RTN_NEXT_RTN_GINDEX	DD	?		;NEXT RTN SAME TYPE-METHOD
_RTN_N_RTNL		DD	?		;NUMBER OF RTNL'S THIS TYPE-NAME
_RTN_RTNL_GINDEX	DD	?		;
_RTN_TYPE_GINDEX	DD	?		;TYPE OF ME
_RTN_ID_GINDEX		DD	?		;<64K IS ID, >=64K IS RESNAME_GINDEX

;_RTN_ALPHA_ORDER	EQU	(_RTN_NEXT_HASH_GINDEX)

RES_TYPE_NAME_STRUCT	ENDS


RTNL_STRUCT	STRUC

_RTNL_NEXT_LANG_GINDEX	DD	?		;NEXT RTNL SAME RTN
_RTNL_NEXT_GINDEX	DD	?		;
_RTNL_FILE_ADDRESS	DD	?		;FILE ADDRESS OF DATA
_RTNL_FILE_SIZE		DD	?		;FILE SIZE OF DATA
_RTNL_FLAGS		DD	?		;LOAD FLAGS - BIT 0 IS 32-BIT FLAG
_RTNL_LANG_ID		DD	?		;LANGUAGE ID
_RTNL_TYPE_ID		DD	?		;TYPE ID IF KNOWN, FOR PROCESSING 16-BIT INTO 32-BIT, VICE VERSA, AND COMPRESSION

RTNL_STRUCT	ENDS

if 0

RESOURCE_STRUCT	STRUC

_RS_NEXT_HASH		DD	?		;NEXT RESOURCE SAME HASH
_RS_NEXT_RESOURCE	DD	?		;NEXT RESOURCE SAME TYPE
_RS_ADDRESS		DD	?		;ADDRESS IN .RES FILE
_RS_LENGTH		DD	?		;LENGTH IN .RES FILE
_RS_FLAGS		DW	?		;LOAD FLAGS
_RS_NAME		DW	?		;ORDINAL OR OFFSET THIS NAME
_RS_TYPE		DW	?		;ORDINAL OR OFFSET OF TYPE

RESOURCE_STRUCT	ENDS

endif

RES_DIRENTRY_STRUCT	STRUC

_RDIRE_NAME_OR_ID	DD	?
_RDIRE_SUBDIR		DD	?

RES_DIRENTRY_STRUCT	ENDS

RES_DIRTABLE_STRUCT	STRUC

_RDIRT_CHARACTER	DD	?
_RDIRT_TIME_DATE	DD	?
_RDIRT_VERSION_MAJOR	DW	?
_RDIRT_VERSION_MINOR	DW	?
_RDIRT_N_BYNAME		DW	?
_RDIRT_N_BYORD		DW	?

RES_DIRTABLE_STRUCT	ENDS

RES_DATAENTRY_STRUCT	STRUC

_RDATE_DATA_RVA		DD	?
_RDATE_DATA_SIZE	DD	?
_RDATE_CODEPAGE		DD	?
_RDATE_RESERVED		DD	?

RES_DATAENTRY_STRUCT	ENDS

