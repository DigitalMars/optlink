.XLIST
TID_VOID		EQU	0
TID_LSTR		EQU	1
TID_DSTR		EQU	2
TID_PSTR		EQU	3
TID_SCHAR		EQU	4
TID_SINT		EQU	5
TID_SLONG		EQU	6
TID_SQUAD		EQU	7
TID_UCHAR		EQU	8
TID_UINT		EQU	9
TID_ULONG		EQU	0AH
TID_UQUAD		EQU	0BH
TID_PCHAR		EQU	0CH
TID_FLOAT		EQU	0DH
TID_TPREAL		EQU	0EH
TID_DOUBLE		EQU	0FH
TID_LDOUBLE		EQU	10H
TID_BCD4		EQU	11H
TID_BCD8		EQU	12H
TID_BCD10		EQU	13H
TID_BCDCOB		EQU	14H
TID_NEAR		EQU	15H
TID_FAR			EQU	16H
TID_SEG			EQU	17H
TID_NEAR386		EQU	18H
TID_FAR386		EQU	19H
TID_CARRAY		EQU	1AH
TID_VLARRAY		EQU	1BH
TID_PARRAY		EQU	1CH
TID_ADESC		EQU	1DH
TID_STRUCT		EQU	1EH
TID_UNION		EQU	1FH
TID_VLSTRUCT		EQU	20H
TID_VLUNION		EQU	21H
TID_ENUM		EQU	22H
TID_FUNCTION		EQU	23H
TID_LABEL		EQU	24H
TID_SET			EQU	25H
TID_TFILE		EQU	26H
TID_BFILE		EQU	27H
TID_BOOL		EQU	28H
TID_PENUM		EQU	29H
TID_PWORD		EQU	2AH
TID_TBYTE		EQU	2BH

TID_SPECIALFUNC		EQU	2DH
TID_CLASS		EQU	2EH

TID_MEMBERPTR		EQU	33H
TID_NREF		EQU	34H
TID_FREF		EQU	35H

TID_NEWMEMBERPTR	EQU	38H



TD_LTDEF_STRUCT		STRUC		;LOCAL TYPE DEFINITION STRUCTURE

_TD_LTDEF_GTYPE		DW	?	;GLOBAL TYPE
_TD_LTDEF_GTYPE_H	DB	?	;HIGH BYTE OF GLOBAL TYPE
_TD_LTDEF_FLAGS		DB	?	;LOCAL TYPE FLAGS
_TD_LTDEF_COMP_PARENT	DW	?	;ITEM CAUSING COMPARE
_TD_LTDEF_MYHASH	DW	?	;HASH FOR THIS TYPE - JUST THIS LEVEL
			;
			;ZEROED UP TO HERE AT CREATE TIME
			;
_TD_LTDEF_TID		DB	?	;TYPE ID
_TD_LTDEF_NAME_LEN	DB	?	;LENGTH OF ASCII NAME
_TD_LTDEF_SIZE		DW	?	;SIZE OF THIS DATA TYPE
_TD_LTDEF_NAME_HASH	DW	?	;HASH OF NAME

TD_LTDEF_STRUCT		ENDS


TD_LTDEF_FLAGS		RECORD	TD_LTDEF_INSTALLED:1, \		;THIS MEANS TYPE HAS BEEN INSTALLED IN GLOBAL TABLE, TID-SIZE==INDEX
				TD_LTDEF_INSTALLING:1, \	;WE ARE CURRENTLY TRYING TO INSTALL THIS GUY
				TD_LTDEF_COMPARING:1

TD_LTDEF_WORD_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_WORD_WORD	DW	?

TD_LTDEF_WORD_STRUCT	ENDS


TD_LTDEF_CLASS_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_CLASS_CLASS	DW	?
;_TD_LTDEF_CLASS_FLAGS	DW	?

TD_LTDEF_CLASS_STRUCT	ENDS


TD_LTDEF_TYPE_STRUCT	STRUC		;ITEM HAS AN XTRA ITEM THAT IS ITSELF A TYPE

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_TYPE_TYPE	DW	?
;_TD_LTDEF_TYPE_TYPE_H	DB	?
;_TD_LTDEF_TYPE_FLAGS	DB	?

TD_LTDEF_TYPE_STRUCT	ENDS


TD_LTDEF_IRANGES_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_RANGE_TYPE	DW	?
;_TD_LTDEF_RANGE_TYPE_H	DB	?
;_TD_LTDEF_RANGE_FLAGS	DB	?
_TD_LTDEF_RANGE_LO_LIM	DD	?
_TD_LTDEF_RANGE_HI_LIM	DD	?

TD_LTDEF_IRANGES_STRUCT	ENDS


TD_LTDEF_TYPE_WORD_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_TW_TYPE	DW	?
;_TD_LTDEF_TW_TYPE_H	DB	?
;_TD_LTDEF_TW_FLAGS	DB	?
_TD_LTDEF_TW_WORD	DW	?

TD_LTDEF_TYPE_WORD_STRUCT	ENDS


TD_LTDEF_TYPE_TYPE_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_TT_TYPE1	DW	?
;_TD_LTDEF_TT_TYPE1_H	DB	?
;_TD_LTDEF_TT_FLAGS1	DB	?
_TD_LTDEF_TT_TYPE2	DW	?
;_TD_LTDEF_TT_TYPE2_H	DB	?
;_TD_LTDEF_TT_FLAGS2	DB	?

TD_LTDEF_TYPE_TYPE_STRUCT	ENDS


TD_LTDEF_TYPE_CLASS_WORD_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_TCW_TYPE	DW	?
;_TD_LTDEF_TCW_TYPE_H	DB	?
;_TD_LTDEF_TCW_TFLAGS	DB	?
_TD_LTDEF_TCW_CLASS	DW	?
;_TD_LTDEF_TCW_CFLAGS	DW	?
_TD_LTDEF_TCW_WORD	DW	?

TD_LTDEF_TYPE_CLASS_WORD_STRUCT	ENDS


TD_LTDEF_ENUM_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_ENUM_TYPE	DW	?
;_TD_LTDEF_ENUM_TYPE_H	DB	?
;_TD_LTDEF_ENUM_TFLAGS	DB	?
_TD_LTDEF_ENUM_LO_LIM	DW	?
_TD_LTDEF_ENUM_HI_LIM	DW	?
_TD_LTDEF_ENUM_MEMBER	DW	?
;_TD_LTDEF_ENUM_MEMBER_H	DB	?
;_TD_LTDEF_ENUM_MFLAGS	DB	?

TD_LTDEF_ENUM_STRUCT	ENDS


TD_LTDEF_STRUCT_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_STRUCT_MEMBER		DW	?
;_TD_LTDEF_STRUCT_MEMBER_H	DB	?
;_TD_LTDEF_STRUCT_FLAGS		DB	?
_TD_LTDEF_STRUCT_SIZE_HI	DW	?

TD_LTDEF_STRUCT_STRUCT	ENDS


TD_LTDEF_SFUNC_STRUCT	STRUC

			DB	SIZE TD_LTDEF_STRUCT DUP(?)
_TD_LTDEF_SFUNC_RTYPE	DW	?		;RETURN TYPE
;_TD_LTDEF_SFUNC_RTYPE_H	DB	?
;_TD_LTDEF_SFUNC_RFLAGS	DB	?
_TD_LTDEF_SFUNC_LANG	DB	?
_TD_LTDEF_SFUNC_FFLAGS	DB	?
_TD_LTDEF_SFUNC_CTYPE	DW	?		;CLASS TYPE
;_TD_LTDEF_SFUNC_CTYPE_H	DB	?
;_TD_LTDEF_SFUNC_CFLAGS	DB	?
_TD_LTDEF_SFUNC_VOFFSET	DW	?		;
_TD_LTDEF_SFUNC_SYMBOL	DW	?

TD_LTDEF_SFUNC_STRUCT	ENDS


TD_GTDEF_STRUCT		STRUC		;LOCAL TYPE DEFINITION STRUCTURE

_TD_GTDEF_NEXT_HASH	DD	?	;NEXT GLOBAL TYPE SAME HASH
_TD_GTDEF_LTDEF		DW	?	;LOCAL TYPE THIS IS BEING DEFINED FROM - ELSE 0 ONCE COMPLETE
_TD_GTDEF_MYHASH	DW	?	;HASH FOR THIS TYPE - JUST THIS LEVEL
_TD_GTDEF_TID		DB	?	;TYPE ID
_TD_GTDEF_NAME_LEN	DB	?	;LENGTH OF ASCII NAME
_TD_GTDEF_SIZE		DW	?	;SIZE OF THIS DATA TYPE
_TD_GTDEF_NAME_HASH	DW	?	;HASH OF NAME

TD_GTDEF_STRUCT		ENDS


TD_GTDEF_WORD_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_WORD_WORD	DW	?

TD_GTDEF_WORD_STRUCT	ENDS


TD_GTDEF_CLASS_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_CLASS_CLASS	DW	?
_TD_GTDEF_CLASS_FLAGS	DW	?

TD_GTDEF_CLASS_STRUCT	ENDS


TD_GTDEF_TYPE_STRUCT	STRUC		;ITEM HAS AN XTRA ITEM THAT IS ITSELF A TYPE

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_TYPE_GTYPE	DD	?

TD_GTDEF_TYPE_STRUCT	ENDS


TD_GTDEF_IRANGES_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_RANGE_GTYPE	DD	?
_TD_GTDEF_RANGE_LO_LIM	DD	?
_TD_GTDEF_RANGE_HI_LIM	DD	?

TD_GTDEF_IRANGES_STRUCT	ENDS


TD_GTDEF_TYPE_WORD_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_TW_GTYPE	DD	?
_TD_GTDEF_TW_WORD	DW	?

TD_GTDEF_TYPE_WORD_STRUCT	ENDS


TD_GTDEF_TYPE_TYPE_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_TT_GTYPE1	DD	?
_TD_GTDEF_TT_GTYPE2	DD	?

TD_GTDEF_TYPE_TYPE_STRUCT	ENDS


TD_GTDEF_TYPE_CLASS_WORD_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_TCW_GTYPE	DD	?
_TD_GTDEF_TCW_CLASS	DW	?
_TD_GTDEF_TCW_CFLAGS	DW	?
_TD_GTDEF_TCW_WORD	DW	?

TD_GTDEF_TYPE_CLASS_WORD_STRUCT	ENDS


TD_GTDEF_ENUM_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_ENUM_GTYPE	DD	?
_TD_GTDEF_ENUM_LO_LIM	DW	?
_TD_GTDEF_ENUM_HI_LIM	DW	?
_TD_GTDEF_ENUM_MEMBER	DD	?

TD_GTDEF_ENUM_STRUCT	ENDS


TD_GTDEF_STRUCT_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_STRUCT_MEMBER	DD	?
_TD_GTDEF_STRUCT_SIZE_HI	DW	?

TD_GTDEF_STRUCT_STRUCT	ENDS


TD_GTDEF_SFUNC_STRUCT	STRUC

			DB	SIZE TD_GTDEF_STRUCT DUP(?)
_TD_GTDEF_SFUNC_RTYPE	DD	?		;RETURN TYPE
_TD_GTDEF_SFUNC_LANG	DB	?
_TD_GTDEF_SFUNC_FFLAGS	DB	?
_TD_GTDEF_SFUNC_CTYPE	DD	?		;CLASS TYPE
_TD_GTDEF_SFUNC_VOFFSET	DW	?		;
_TD_GTDEF_SFUNC_SYMBOL	DW	?

TD_GTDEF_SFUNC_STRUCT	ENDS


TD_LSMEM_STRUCT		STRUC

_TD_LSMEM_INFO_BYTE	DB	?		;
_TD_LSMEM_NAME_LEN	DB	?		;
_TD_LSMEM_FLAGS		DW	?		;HAS_TYPE
_TD_LSMEM_DWORD		DD	?		;NEW OFFSET, TYPE, OR VALUE FOR ENUM
_TD_LSMEM_NAME_HASH	DW	?

TD_LSMEM_STRUCT		ENDS


TD_LSMEM_RECORD		RECORD	TD_LSMEM_HAS_TYPE:1,TD_LSMEM_IS_ENUM:1,TD_LSMEM_IS_NEWOFFSET:1


TD_LSMEM_1ST_XTRA_STRUCT	STRUC

_XTD_LSMEM1_GSMEM	DW	?		;MY EQUIVALENT GINDEX
_XTD_LSMEM1_GSMEM_H	DB	?		;
_XTD_LSMEM1_FLAGS	DB	?		;
_XTD_LSMEM1_COMP_PARENT	DW	?
_XTD_LSMEM1_COUNT	DW	?		;# OF MEMBERS THIS STRUCTURE
_XTD_LSMEM1_HASH	DW	?		;HASH OF MY MEMBERS

TD_LSMEM_1ST_XTRA_STRUCT	ENDS

_TD_LSMEM1_GSMEM	EQU	(_XTD_LSMEM1_GSMEM - SIZE TD_LSMEM_1ST_XTRA_STRUCT)
_TD_LSMEM1_GSMEM_H	EQU	(_XTD_LSMEM1_GSMEM_H - SIZE TD_LSMEM_1ST_XTRA_STRUCT)
_TD_LSMEM1_FLAGS	EQU	(_XTD_LSMEM1_FLAGS - SIZE TD_LSMEM_1ST_XTRA_STRUCT)
_TD_LSMEM1_COMP_PARENT	EQU	(_XTD_LSMEM1_COMP_PARENT - SIZE TD_LSMEM_1ST_XTRA_STRUCT)
_TD_LSMEM1_COUNT	EQU	(_XTD_LSMEM1_COUNT - SIZE TD_LSMEM_1ST_XTRA_STRUCT)
_TD_LSMEM1_HASH		EQU	(_XTD_LSMEM1_HASH - SIZE TD_LSMEM_1ST_XTRA_STRUCT)


TD_LSMEM1_RECORD	RECORD	TD_LSMEM1_COMPARING:1,TD_LSMEM1_INSTALLING:1,TD_LSMEM1_INSTALLED:1


TD_GSMEM_STRUCT		STRUC

_TD_GSMEM_INFO_BYTE	DB	?		;
_TD_GSMEM_NAME_LEN	DB	?		;
_TD_GSMEM_FLAGS		DW	?		;HAS TYPE
_TD_GSMEM_DWORD		DD	?		;NEW OFFSET, TYPE, OR VALUE FOR ENUM
_TD_GSMEM_NAME_HASH	DW	?

TD_GSMEM_STRUCT		ENDS


TD_GSMEM_1ST_XTRA_STRUCT	STRUC

_XTD_GSMEM1_NEXT_HASH	DD	?
_XTD_GSMEM1_LSMEM	DW	?		;LSMEM THIS IS BEING DEFINED AS
_XTD_GSMEM1_COUNT	DW	?		;# OF MEMBERS THIS STRUCTURE
_XTD_GSMEM1_HASH	DW	?		;HASH OF MY MEMBERS

TD_GSMEM_1ST_XTRA_STRUCT	ENDS

_TD_GSMEM1_NEXT_HASH	EQU	(_XTD_GSMEM1_NEXT_HASH - SIZE TD_GSMEM_1ST_XTRA_STRUCT)
_TD_GSMEM1_LSMEM	EQU	(_XTD_GSMEM1_LSMEM - SIZE TD_GSMEM_1ST_XTRA_STRUCT)
_TD_GSMEM1_COUNT	EQU	(_XTD_GSMEM1_COUNT - SIZE TD_GSMEM_1ST_XTRA_STRUCT)
_TD_GSMEM1_HASH		EQU	(_XTD_GSMEM1_HASH - SIZE TD_GSMEM_1ST_XTRA_STRUCT)


TD_LCLASS_STRUCT	STRUC

_TD_LCLASS_GCLASS	DW	?		;GLOBAL EQUIVALENT TO ME
_TD_LCLASS_FLAGS	DW	?		;HAS MEMBER HASH BEEN TAKEN INTO ACCOUNT?
_TD_LCLASS_COMP_PARENT	DW	?		;PARENT ITEM CAUSING COMPARE
_TD_LCLASS_HASH		DW	?		;OVERALL HASH
_TD_LCLASS_SCOPE	DW	?		;OWNING LSCOPE
_TD_LCLASS_N_PARENTS	DW	?		;ZEROD UP TO HERE, THIS IS FIRST INITIALIZED BY TDBG_CLASS
_TD_LCLASS_MEMBER	DW	?
_TD_LCLASS_VPTR		DW	?
_TD_LCLASS_INFO_BITS	DB	?
_TD_LCLASS_NAME_LEN	DB	?
_TD_LCLASS_NAME_HASH	DW	?

TD_LCLASS_STRUCT	ENDS


TD_LCLASS_RECORD	RECORD	TD_LCLASS_NEED_MEMBER_HASH:1,TD_LCLASS_COMPARING:1,TD_LCLASS_INSTALLING:1,TD_LCLASS_INSTALLED:1


TD_GCLASS_STRUCT	STRUC

_TD_GCLASS_NEXT_HASH	DW	?		;NEXT CLASS SAME HASH
_TD_GCLASS_LCLASS	DW	?		;LCLASS THIS IS BEING DEFINED AS, ELSE 0
_TD_GCLASS_HASH		DW	?		;OVERALL HASH
_TD_GCLASS_SCOPE	DW	?		;OWNING LSCOPE
_TD_GCLASS_N_PARENTS	DW	?		;ZEROD UP TO HERE, THIS IS FIRST INITIALIZED BY TDBG_CLASS
_TD_GCLASS_GSMEM	DD	?		;STRUCTURE MEMBER I POINT TO
_TD_GCLASS_VPTR		DW	?
_TD_GCLASS_INFO_BITS	DB	?
_TD_GCLASS_NAME_LEN	DB	?
_TD_GCLASS_NAME_HASH	DW	?

TD_GCLASS_STRUCT	ENDS


