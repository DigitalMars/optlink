.XLIST

AREA_STRUCT	STRUC

_AREA_FIRST_SECT_GINDEX		DD	?	;FIRST SECTION THIS AREA
_AREA_NEXT_AREA_GINDEX 		DD	?	;NEXT AREA SAME LEVEL
_AREA_PARENT_SECT_GINDEX	DD	?	;
_AREA_LEVEL			DD	?	;
_AREA_BASE_ADDRESS		DD	?	;
_AREA_MAX_ADDRESS		DD	?	;EQU	_AREA_LAST_SECTION
_AREA_LAST_OUTFILE_GINDEX	DD	?	;OUTFILE LAST USED BY THIS AREA
_AREA_LAST_SECT_GINDEX		DD	?	;LAST SECTION THIS AREA
_AREA_FORCE_SIZE		DD	?	;SIZE CAN BE FORCED BY CMDLINE

AREA_STRUCT	ENDS

SECTION_STRUCT	STRUC

_SECT_NEXT_SECT_GINDEX		DD	?	;NEXT SECTION SAME AREA
_SECT_NEXT_SECTION_GINDEX	DD	?	;NEXT SECTION REFERENCED ORDER...
_SECT_RELOCS			DD	?	;# RELOCS THIS SECTION
_SECT_NUMBER			DD	?	;THIS SECTION NUMBER

_SECT_FIRST_AREA_GINDEX		DD	?	;FIRST AREA UNDER THIS SECTION
_SECT_FIRST_SEGMOD_GINDEX	DD	?	;FIRST SEGMOD ASSIGNED THIS SECTION

_SECT_NAME			DD	?	;LNAME POINTER IF NAME ASSIGNED

_SECT_OUTFILE_GINDEX		DD	?	;_OF_ POINTER TO OUTPUT FILE

_SECT_FLAGS			DB	?	;PRELOAD, SLRPACKED, CACHEABLE. RELOCS, CACHED, IN-MEMORY
				DB	?
				DW	?

_SECT_FILE_SIZE 		DD	?	;SECTION SIZE ON DISK IN PARAGRAPHS (INCLUDING RELOCS)
_SECT_PARENT_AREA_GINDEX	DD	?	;AREA I BELONG IN

_SECT_CHILDREN			DD	?	;NUMBER OF CHILD SECTIONS
_SECT_LAST_CLONE_GINDEX		DD	?	;FOR AUTOSECTION USE

_SECT_FILE_ADDRESS		DD	?	;OFFSET IN OUTFILE

_SECT_LAST_SEGMOD_GINDEX	DD	?	;LAST SEGMOD ASSIGNED THIS SECTION
_SECT_LAST_AREA_GINDEX	 	DD	?	;LAST AREA UNDER THIS SECTION

_SECT_BASE_ADDRESS	EQU	_SECT_LAST_SEGMOD_GINDEX
_SECT_MAX_ADDRESS	EQU	_SECT_CHILDREN

SECTION_STRUCT	ENDS

.LIST

