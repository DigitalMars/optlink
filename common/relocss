.XLIST
RELOC_STRUCT		STRUC

_RL_NEXT_HASH_GINDEX	DD	?	;NEXT RELOC SAME HASH
_RL_NEXT_RELOC_GINDEX	DD	?	;RELOC ORDER...
_RL_SEG_OFFSET		DD	?	;OFFSET IN CURRENT SEGMENT OF LAST REFERENCE
_RL_TARG_OFFSET		DD	?	;OFFSET FROM TARGET SEGMENT
_RL_OS2_NUMBER		DD	?	;TARGET SEGMENT #
_RL_FLAGS		DD	?	;RELOC TYPE JUNK

RELOC_STRUCT		ENDS

_RL_IMP_OFFSET	EQU	_RL_TARG_OFFSET
_RL_IMP_MODULE	EQU	_RL_OS2_NUMBER

.LIST

