.XLIST

LIBRARY_STRUCT		STRUC

_LS_NEXT_LIB_GINDEX	DD	?	;NEXT LIBRARY	PROBABLY FOR LRU
_LS_PREV_LIB_GINDEX	DD	?	;PREVIOUS LIBRARY
_LS_NEXT_RLIB_GINDEX	DD	?	;NEXT LIBRARY - REQUESTED ORDER
_LS_PREV_RLIB_GINDEX	DD	?	;I THINK NOT USED
if	fgh_inthreads
_LS_NEXT_TRLIB_GINDEX	DD	?	;NEXT LIBRARY - REQUESTED ORDER FOR THIS THREAD
_LS_NEXT_TRLIB_BLOCK	DD	?	;NEXT BLOCK FOR LIBREAD TO READ
_LS_NEXT_REQUEST_BLOCK	DD	?	;NEXT REQUEST BLOCK STORAGE SPOT
;_LS_HANDLE_SEM		GLOBALSEM_STRUCT<>	;SEMAPHORE CONTROLLING ACCESS TO HANDLE
endif

_LS_BLOCKS_LEFT 	DD	?	;# OF BLOCKS YET TO READ
_LS_BLOCKS		DD	?	;TOTAL # OF PAGE_SIZE BLOCKS

_LS_LIBNUM		DD	?	;CV LIBRARY NUMBER
_LS_NEXT_CV_GINDEX	DD	?	;IN CODEVIEW ORDER...

if	fgh_inthreads
_LS_OS2_BLOCKS_REQUESTED	DD	?	;# OF BLOCKS FOR OS2 THREAD TO READ
_LS_THREAD_LOCALS	DD	?	;OWNING READ-THREAD
endif

_LS_MODULES		DD	?	;TOTAL # OF MODULES THIS LIB
_LS_MODULES_LEFT	DD	?	;# OF MODULES NOT READ IN		- I THINK NOT USED

_LS_SELECTED_MODULES	DD	?	;# UNREAD SELECTED MODULES
_LS_HANDLE		DD	?	;FILE HANDLE

_LS_BLOCK_TABLE 	DD	4 DUP(?);PTR TO BLOCK TABLE - 4 BYTES PER BLOCK (MAYBE SEGMENT LIST IF TOO MANY BLOCKS)
					;
					;DD	BLOCK_ADDRESS IF READ
					;DD	# OF MODULES UNPROCESSED IN THIS BLOCK
					;
_LS_FILE_POSITION	DD	?	;CURRENT FILE-HANDLE POSITION
_LS_MODULE_PTRS 	DD	?	;PTR TO MODULE OFFSETS - 4 BYTE OFFSETS.  THIS IS PTR TO LIST OF BLOCKS IF >PAGE_SIZE/8
_LS_FILE_LIST_GINDEX	DD	?	;PTR TO ORIGINAL FILE_LIST ENTRY
_LS_FIRST_EXTRN_GINDEX	DD	?	;FIRST REQUESTED SYMBOL FOR THIS LIBRARY
_LS_LAST_EXTRN_GINDEX	DD	?	;LAST REQUESTED SYMBOL FOR THIS

LIBRARY_STRUCT		ENDS

.LIST

